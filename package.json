{
  "name": "imp-splen-site",
  "version": "1.0.0",
  "description": "Imperial Splendour Site",
  "author": "Sophie Au <sophie@sophieau.com>",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "prod": "gatsby build && gatsby serve",
    "checks": "tsc && npm run lint",
    "format": "prettier --write '{src,data}/**/*.{tsx,ts}'",
    "lint": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts' --ignore-pattern 'src/assets/*' && stylelint 'src/**/*.styles.ts' --rd --risd",
    "stylelint": "stylelint '.' --rd --risd",
    "lint:fix": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts'  --ignore-pattern 'src/assets/*' --fix && stylelint 'src/**/*.styles.ts' --rd --risd --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@reach/router": "^1.3.4",
    "core-js": "^3.23.2",
    "gatsby": "^3.14.6",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-feed": "^3.14.0",
    "gatsby-plugin-linaria": "^2.3.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-netlify-cms": "^5.14.0",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sharp": "^3.14.3",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-typescript": "^3.14.0",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-remark-social-image": "^1.1.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "gatsby-transformer-yaml": "^3.14.0",
    "isomorphic-fetch": "^3.0.0",
    "linaria": "^2.3.1",
    "netlify-cms-app": "^2.15.72",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^6.0.3"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.13.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.7.4"
  }
}
