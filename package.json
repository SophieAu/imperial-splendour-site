{
  "name": "imp-splen-site",
  "version": "1.0.0",
  "description": "Imperial Splendour Site",
  "author": "Sophie Au <sophie@sophieau.com>",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "prod": "gatsby build && gatsby serve",
    "checks": "tsc && npm run lint",
    "format": "prettier --write '{src,data}/**/*.{tsx,ts}'",
    "lint": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts' --ignore-pattern 'src/assets/*' && stylelint 'src/**/*.styles.ts' --rd --risd",
    "stylelint": "stylelint '.' --rd --risd",
    "lint:fix": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts'  --ignore-pattern 'src/assets/*' --fix && stylelint 'src/**/*.styles.ts' --rd --risd --fix"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "gatsby": "^2.24.67",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-feed": "^2.5.13",
    "gatsby-plugin-linaria": "^2.1.0",
    "gatsby-plugin-manifest": "^2.4.33",
    "gatsby-plugin-netlify-cms": "^4.3.15",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-plugin-typescript": "^2.4.25",
    "gatsby-remark-images": "^3.3.32",
    "gatsby-remark-social-image": "^1.1.0",
    "gatsby-source-filesystem": "^2.3.37",
    "gatsby-transformer-remark": "^2.8.47",
    "gatsby-transformer-sharp": "^2.5.12",
    "gatsby-transformer-yaml": "^2.4.10",
    "linaria": "^2.0.0",
    "netlify-cms-app": "^2.12.25",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.0.3"
  }
}
