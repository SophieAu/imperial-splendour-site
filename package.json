{
  "name": "imp-splen-site",
  "version": "1.0.0",
  "description": "Imperial Splendour Site",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "prod": "gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "checks": "tsc && npm run lint",
    "format": "prettier --write '{src,data}/**/*.{tsx,ts}'",
    "lint": "eslint '{src,data}/**/*.{ts,tsx}' --ignore-pattern '*.d.ts'",
    "lint:fix": "eslint '{src,data}/**/*.{ts,tsx}' --ignore-pattern '*.d.ts' --fix"
  },
  "author": "Sophie Au <sophie@sophieau.com>",
  "license": "MIT",
  "dependencies": {
    "gatsby": "^2.18.25",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-feed": "^2.3.26",
    "gatsby-plugin-manifest": "^2.2.40",
    "gatsby-plugin-netlify-cms": "^4.1.37",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.4.4",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-plugin-typescript": "^2.1.26",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.13",
    "gatsby-transformer-yaml": "^2.2.23",
    "netlify-cms-app": "^2.11.5",
    "node-sass": "^4.13.1",
    "prismjs": "^1.19.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "prettier": "1.19.1",
    "typescript": "^3.7.5"
  }
}
